	__path_type = true;
	new = function(_, t)
		if type(t) ~= 'table' then
			std.err("Wrong path argument. Use {} as path argument.", 2)
		end
		local n, s, w = t[1], t[2], t[3]

		if not w then
			s, w = n, s
			n = nil
		end

		local new = {
			before = s;
			walk = w;
		}

		for k, v in std.pairs(t) do
			if type(k) == 'string' then
				new[k] = v
