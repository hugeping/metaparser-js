			w = w()
		end
		if disabled(w) or closed(w) then
			return false
		end
		if s.after ~= nil and visited(w) then
			return std.call(s, 'after')
		end
		return std.call(s, 'before')
	end;
	onwalk = function(s, _)
		local w = s.walk
		if type(w) == 'function' then
			w = w()
		end
		if not disabled(w) and not closed(w) then
			walk(w)
		end
